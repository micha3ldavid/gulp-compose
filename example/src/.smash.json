{
  "root": "./example/src",
  "files": [
    "{root}/one.js",
    "{root}/two.js",
    "{root}/three.js",
    "./src/core.js"
  ],
  "output": {
    "name": "example",
    "file": "example.js",
    "dest": "./example/dist",
    "wrap": "js-environment-wrapper",
    "deps": {
      "core": {
        "reference": "gulpSmash",
        "require": "gulp-smash/core",
        "define": "gulp-smash/core"
      }
    }
  },
  "gulp-minify": {
    "ext": {
      "min": ".min.js"
    }
  }
}
