{
  "root": "./example/src",
  "files": [
    "{root}/one.js",
    "{root}/two.js",
    "{root}/three.js",
    "./src/core.js"
  ],
  "output": {
    "file": "example.js",
    "dest": "./example/dist",
    "wrap": "environment.js",
    "vars": {
      "namespace": "example",
      "dependencies": {
        "jquery": {
          "reference": "jQuery",
          "require": "libs/jquery",
          "define": "libs/jquery"
        }
      }
    }
  },
  "gulp-minify": {
    "ext": {
      "min": ".min.js"
    }
  }
}
