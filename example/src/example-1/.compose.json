{
  "root": "./example/src/example-1",
  "files": [
    "{root}/*.js"
  ],
  "output": {
    "file": "index.js",
    "dest": "./example/src",
    "wrap": "environment.js",
    "vars": {
      "namespace": "example_1",
      "dependencies": {
        "jquery": {
          "reference": "$",
          "require": "jquery",
          "define": "jquery"
        }
      }
    },
    "distribution": {
      "file": "example-1.js",
      "dest": "./example/dist",
      "pipe": {
        "babelify": {},
        "minify": {
          "ext": {
            "min": ".min.js"
          }
        }
      }
    }
  }
}
